  "license": "MIT",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "dev": "nodemon -delay 500ms --watch 'src/' --exec 'ts-node -r tsconfig-paths/register --files src/server.ts' -e ts",
    "husky:prepare": "husky install",
    "test": "jest --passWithNoTests",
    "test:watch": "yarn test --watch",
    "test:staged": "yarn test --findRelatedTests",
    "test:push": "yarn test --coverage"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.4",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^18.11.17",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@aws-sdk/util-dynamodb": "^3.245.0",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "dynamoose": "^3.1.0",
    "express": "^4.18.2",
    "joi": "^17.7.0",
    "jsonwebtoken": "^9.0.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.7.0"
  }
}
